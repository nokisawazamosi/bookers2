<%= render 'books/list', book: @book, user: @user %>
<%#= render 'list', user: @user %>


<table>
    <tr><% @books.each do |book| %></tr>
      <td><%= image_tag book.user.get_profile_image(80,80) %></td>
      <td><%= link_to book.title,book_path(book.id) %></td>
      <td><%= book.body %></td>
    <tr><% end %></tr>
</table>


